/**index.wxss**/
.container{
  display: flex;
  flex-direction: column;
}
.headContainer{
 width:100%;
 height:160rpx;
}

.select-contianer{
  height: 80rpx;
  border: 2rpx;
  border-radius: 10rpx;
  background: #DDDDDD;
  text-align: center;
}

.company-text{
  height:40rpx;
  width: 40rpx;
  font-size: 60rpx;
  font-family: '微软雅黑';
  bottom: 60rpx;
  left:-10rpx;
}
.select-contianer image{
  width: 25rpx;
  height: 25rpx;
}

 .plate{
   height: 140rpx;
   background: #FFF;
 }

 .plate-item{
  display: flex;
  float:left;
  position: relative;
  width: 100rpx;
  height:180rpx;
  margin-top:38rpx; 
  margin-left:42rpx;
}

.plate-img image{
  width: 80rpx;
  height: 80rpx;
  margin-left: -12rpx;
}
.plate-info{
  position: absolute;
  height:40rpx;
  font-size: 28rpx;
  font-family: '微软雅黑';
  bottom: 60rpx;
  left:-10rpx;
}

